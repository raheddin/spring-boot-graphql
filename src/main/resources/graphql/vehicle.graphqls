type Vehicle {
    id: ID!,
    type: String!,
    modelCode: String,
    brandName: String,
    launchDate: String
}

input VehicleDto {
    type: String!,
    modelCode: String!,
    brandName: String
}

type Query {
    getVehicles(type: String):[Vehicle]
    getVehicle(id: Int):Vehicle
    getAllVehicles:[Vehicle]
}

type Mutation {
    createVehicle(vehicle: VehicleDto):Vehicle
}